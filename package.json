{
  "name": "ng2-easyui",
  "version": "0.1.38",
  "description": "",
  "main": "./bundles/ng2-easyui.js",
  "module": "./ng2-easyui.es5.js",
  "es2015": "./ng2-easyui.js",
  "typings": "./ng2-easyui.d.ts",
  "scripts": {
    "build:example": "ng build --app example --prod --no-extract-license --build-optimizer",
    "clean:lib": "rimraf dist/grid",
    "build:app": "ng build --app app --no-extract-license --prod --build-optimizer",
    "buildpkg": "ng-packagr -p ng-package.json",
    "clean": "rimraf out-tsc && rimraf dist",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copycss": "cpr assets/ dist/assets",
    "publish:lib": "npm run clean && npm run build && npm run copycss",
    "bundle-report:example": "webpack-bundle-analyzer out/example/stats.json",
    "bundle-report:app": "webpack-bundle-analyzer out/app/stats.json",
    "mirror:npm": "npm config set registry http://registry.npmjs.org",
    "mirror:taobao": "npm config set registry https://registry.npm.taobao.org"
  },
  "keywords": [
    "angular2",
    "angular4",
    "ng2",
    "easy",
    "ui"
  ],
  "author": "qiao.li",
  "license": "MIT",
